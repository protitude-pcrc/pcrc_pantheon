{
    "name": "drupal/legacy-project",
    "description": "Project template for Drupal 8 projects with composer following drupal/drupal layout",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
          "type": "git",
          "url": "https://github.com/protitude/file_download.git"
        }
    ],
    "require": {
        "acquia/blt": "^13",
        "acquia/blt-behat": "^1.3",
        "bower-asset/masonry": "^4.2",
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "~1.0",
        "drupal/addtoany": "^2.0.0",
        "drupal/admin_toolbar": "^3.2.0",
        "drupal/auto_entitylabel": "^3.0.0-beta3",
        "drupal/blazy": "^2.0.14",
        "drupal/captcha": "^1.0.0-beta1",
        "drupal/ckeditor_codemirror": "^2.0.0",
        "drupal/config_devel": "^1.0.0",
        "drupal/context": "^4.0.0-beta2",
        "drupal/context_profile_role": "^1.3.0",
        "drupal/core-composer-scaffold": "^9.5.9",
        "drupal/core-project-message": "^9.5.9",
        "drupal/core-recommended": "^9.5.9",
        "drupal/csv_serialization": "^2.1.0",
        "drupal/ctools": "^4.0.0",
        "drupal/devel": "^5.1.1",
        "drupal/dropzonejs": "^2.0.0-alpha1",
        "drupal/ds": "^3.9.0",
        "drupal/editor_advanced_link": "^2.2.4",
        "drupal/embed": "^1.0.0",
        "drupal/empty_fields": "^1.0.0",
        "drupal/entity": "^1.0.0-alpha4",
        "drupal/entity_browser": "^2.9.0",
        "drupal/entity_clone": "^2.0.0",
        "drupal/entity_embed": "^1.4.0",
        "drupal/entityqueue": "^1.0.0-alpha7",
        "drupal/facets": "^2.0",
        "drupal/feeds": "^3.0.0-alpha1",
        "drupal/field_group": "^3.1.0",
        "drupal/file_browser": "^1.3.0",
        "drupal/file_entity": "^2.0.0-beta7",
        "drupal/fontyourface": "4.0.x-dev@dev",
        "drupal/google_analytics": "^4.0.0",
        "drupal/imagemagick": "^3.4",
        "drupal/libraries": "^4.0.0",
        "drupal/link_attributes": "^2.1.0",
        "drupal/linkit": "^5.0.0",
        "drupal/menu_block": "^1.4.0",
        "drupal/name": "^1.0.0-rc1",
        "drupal/pathauto": "^1.6.0",
        "drupal/pathologic": "^2.0.0",
        "drupal/recaptcha": "^3.1.0",
        "drupal/redirect": "^1.8",
        "drupal/search_api": "^1.8.0",
        "drupal/shortcode": "^2.0.0",
        "drupal/simple_block": "^1.0.0-beta1",
        "drupal/simple_sitemap": "^4.1",
        "drupal/smart_trim": "^2.1.0",
        "drupal/smtp": "^1.0.0-beta4",
        "drupal/superfish": "^1.2.0",
        "drupal/token": "^1.0.0",
        "drupal/video_embed_field": "^2.0.0",
        "drupal/views_data_export": "^1.0.0-beta1",
        "drupal/views_year_filter": "^2.0",
        "drupal/webform": "^6.0.4",
        "drupal/webform_views": "^5.1.0",
        "drush/drush": "^11",
        "npm-asset/dropzone": "^5.9",
        "npm-asset/imagesloaded": "^5.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "pcrc/file_download": "dev-main",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "wikimedia/composer-merge-plugin": true,
            "acquia/blt": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            },
            "file-mapping": {
                "[web-root]/README.txt": false,
                "[web-root]/sites/README.txt": false,
                "[web-root]/example.gitignore": false,
                "[web-root]/sites/example.settings.local.php": false,
                "[web-root]/sites/example.sites.php": false,
                "[web-root]/modules/README.txt": false,
                "[web-root]/profiles/README.txt": false,
                "[web-root]/themes/README.txt": false,
                "[web-root]/INSTALL.txt": false
            }
        },
        "merge-plugin": {
            "include": [
                "docroot/modules/contrib/webform/composer.libraries.json"
            ],
            "recurse": true,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-replace": true,
            "merge-scripts": false
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Example\\": "blt/src/"
        }
    },
    "require-dev": {
        "behat/mink": "^1.8",
        "drupal/core-dev": "^8.0@beta",
        "drupal/upgrade_status": "^4.0"
    },
    "scripts": {
        "post-drupal-scaffold-cmd": [
            "patch -p1 < patches/htaccess-ssl.patch",
            "./blt/file_download.sh"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    }
}
